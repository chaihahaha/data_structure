#include<iostream>
using namespace std;
typedef int ElemType;
class StackNode
{
public:
        ElemType data;
        StackNode * next;
};
class Stack
{
public:
        void InitStack() {
                this->top = NULL;
        }
        void Push(ElemType x) {
                StackNode * p = new StackNode();
                p->data = x;
                p->next = top;
                top = p;
        }
        ElemType Pop() {
                ElemType ret = top->data;
                top = top->next;
                return ret;
        }
        bool IsEmpty() {
                return !top;
        }
        ElemType GetTop() {
                return top->data;
        }
        StackNode * top;
};
