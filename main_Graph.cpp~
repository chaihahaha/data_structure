#include "MGraph.cpp"
int main(){
    MGraph* g=new MGraph(4,4);
    for(int i = 0; i < g->vexnum; i++) {
        g->vexs[i] = i;
        for(int j = 0; j < g->vexnum; j++) {
            g->arcs[i][j].adj = 1;
        }
    }
    int arcs[4][4]={{0,1,0,0},{0,0,1,1},{0,1,0,1},{1,0,1,0}};
    g->InitMGraph(arcs);
    for(int i = 0; i < g->vexnum; i++) {
        for(int j = 0; j < g->vexnum; j++) {
            cout<<g->arcs[i][j].adj<<"  ";
        }
        cout<<endl;
    }
    return 0;
}
